<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.project_entertainment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.PlayVideo_Activity_TrangChu"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".Activity.HaiKich_Activity" />
        <activity android:name=".Activity.CaNhac_Activity" />
        <activity android:name=".Activity.TheThao_Activity" />
        <activity android:name=".Activity.Phim_Activity"></activity>
    </application>

</manifest>